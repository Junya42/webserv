server
{
	listen: 65535

	index: default/webserv.html
	server_name: webserv.42.com
	cgi: .sh /usr/bin/bash .py /usr/bin/python3

	force_login: true
	enable_redirect: true

	location: /user
	{
		root: www/html
		method_accept: GET
	}
	location: /login
	{
		root: www
		method_accept: GET
	}
	location: /html
	{
		root: www
		method_accept: GET POST
	}
	location: /upload
	{
		root: www/html
		method_accept: GET POST
	}
	location: /cgi-bin
	{
		root: www
	}
	location: /tmp
	{
		root:
		method_accept: GET POST DELETE
	}
}

